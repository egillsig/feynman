unit m ;;
def ft = 0.3048 m ;;
10 ft ;;

unit ft ;;
def m = 3.2808399 ft ;;
10 m ;;

unit m ;;
unit kg ;;
unit s ;;
unit K ;;
unit J = kg m^2 s^-2;;
def boltzmann = 1.38064852 10^-23 m m kg / (s s K) ;;
def avg_kinetic_energy[T: K]: J = (3/2) boltzmann T ;;
def Celcius[c: 1]: K = (c + 273.15) K ;;
avg_kinetic_energy[Celcius[20]] ;;

unit delta_K ;;
def delta_C = delta_K ;;
def delta_F = (5/9) delta_K ;;
def expansion[alpha, delta_T: delta_K, length: m]: m = alpha delta_T length
